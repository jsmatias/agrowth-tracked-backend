{
  "defaultSeverity": "error",
  "extends": ["tslint:latest", "tslint-config-airbnb", "tslint-config-prettier"],
  "jsRules": {},
  "rules": {
    "import-name": [
      true,
      {
        "bcryptjs": "bcrypt",
        "jsonwebtoken": "jwt",
        "v4": "uuidV4"
      }
    ],
    "no-unused-variable": false,
    "quotemark": [true, "single", "jsx-double", "avoid-escape", "avoid-template"],
    "variable-name": [true, "allow-pascal-case"],
    "no-submodule-imports": [true, ["uuid/v4"]]
  },
  "rulesDirectory": []
}
